Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Q", "[", "z_", "]"}], ":=", 
   RowBox[{
    FractionBox["1", "2"], 
    RowBox[{"Erfc", "[", 
     FractionBox["z", 
      SqrtBox["2"]], "]"}]}]}], ";", 
  RowBox[{
   RowBox[{"f\[Alpha]\[Mu]", "[", 
    RowBox[{"\[Gamma]_", ",", "\[Gamma]m_", ",", "\[Mu]_"}], "]"}], ":=", " ", 
   RowBox[{
    FractionBox[
     RowBox[{
      SuperscriptBox["\[Mu]", 
       RowBox[{"\[Mu]", " "}]], 
      SuperscriptBox["\[Gamma]", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"\[Mu]", " ", "-", "1"}], ")"}], " "}]]}], 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        SuperscriptBox["10", 
         RowBox[{
          FractionBox["\[Gamma]m", "10"], " "}]], ")"}], "\[Mu]"], 
      RowBox[{"Gamma", "[", "\[Mu]", "]"}]}]], 
    RowBox[{"Exp", "[", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{"\[Mu]", " ", "\[Gamma]"}], 
       SuperscriptBox["10", 
        RowBox[{
         FractionBox["\[Gamma]m", "10"], " "}]]]}], "]"}]}]}], ";", 
  RowBox[{
   RowBox[{"pdf\[Omega]", "[", 
    RowBox[{"\[Gamma]_", ",", "\[Gamma]m_", ",", "\[Alpha]_", ",", "\[Mu]_"}],
     "]"}], ":=", 
   FractionBox[
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "\[Mu]"}], " ", 
       SuperscriptBox["\[Gamma]", 
        RowBox[{"\[Alpha]", "/", "2"}]], " ", 
       SuperscriptBox[
        RowBox[{"(", 
         FractionBox[
          RowBox[{
           SuperscriptBox["\[Mu]", 
            RowBox[{"2", "/", "\[Alpha]"}]], " ", 
           RowBox[{"(", 
            SuperscriptBox["10", 
             RowBox[{
              FractionBox["\[Gamma]m", "10"], " "}]], ")"}], " ", 
           RowBox[{"Gamma", "[", "\[Mu]", "]"}]}], 
          RowBox[{"Gamma", "[", 
           RowBox[{
            FractionBox["2", "\[Alpha]"], "+", "\[Mu]"}], "]"}]], ")"}], 
        RowBox[{
         RowBox[{"-", "\[Alpha]"}], "/", "2"}]]}]], " ", "\[Alpha]", " ", 
     SuperscriptBox["\[Mu]", "\[Mu]"], " ", 
     SuperscriptBox["\[Gamma]", 
      RowBox[{
       RowBox[{"-", "1"}], "+", 
       FractionBox[
        RowBox[{"\[Alpha]", " ", "\[Mu]"}], "2"]}]], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       FractionBox[
        RowBox[{
         SuperscriptBox["\[Mu]", 
          RowBox[{"2", "/", "\[Alpha]"}]], " ", 
         RowBox[{"(", 
          SuperscriptBox["10", 
           RowBox[{
            FractionBox["\[Gamma]m", "10"], " "}]], ")"}], " ", 
         RowBox[{"Gamma", "[", "\[Mu]", "]"}]}], 
        RowBox[{"Gamma", "[", 
         RowBox[{
          FractionBox["2", "\[Alpha]"], "+", "\[Mu]"}], "]"}]], ")"}], 
      RowBox[{"-", 
       FractionBox[
        RowBox[{"\[Alpha]", " ", "\[Mu]"}], "2"]}]]}], 
    RowBox[{"2", " ", 
     RowBox[{"Gamma", "[", "\[Mu]", "]"}]}]]}], ";", 
  RowBox[{
   RowBox[{"probErrorPSK", "[", "\[Gamma]_", "]"}], ":=", 
   RowBox[{"Q", "[", 
    SqrtBox[
     RowBox[{"2", " ", "\[Gamma]"}]], "]"}]}], ";", 
  RowBox[{
   RowBox[{"BERBPSK", "[", 
    RowBox[{"\[Gamma]m_", ",", "\[Alpha]_", ",", "\[Mu]_"}], "]"}], " ", ":=",
    " ", 
   RowBox[{"Re", "[", 
    RowBox[{"Integrate", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"probErrorPSK", "[", "\[Gamma]", "]"}], "\[Times]", 
       RowBox[{"pdf\[Omega]", "[", 
        RowBox[{
        "\[Gamma]", ",", " ", "\[Gamma]m", ",", "\[Alpha]", ",", "\[Mu]"}], 
        "]"}]}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"\[Gamma]", ",", "0", ",", "\[Infinity]"}], "}"}]}], "]"}], 
    "]"}]}], ";", 
  RowBox[{
   RowBox[{"PNmPSK", "[", 
    RowBox[{
    "\[Gamma]m_", ",", "\[Alpha]_", ",", "\[Mu]_", ",", "N_", ",", "m_"}], 
    "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     FractionBox[
      RowBox[{"N", "!"}], 
      RowBox[{
       RowBox[{"m", "!"}], 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"N", "-", "m"}], ")"}], "!"}]}]], ")"}], "\[Times]", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "-", 
       RowBox[{"BERBPSK", "[", 
        RowBox[{"\[Gamma]m", ",", "\[Alpha]", ",", "\[Mu]"}], "]"}]}], ")"}], 
     RowBox[{"N", "-", "m", " "}]], "\[Times]", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"BERBPSK", "[", 
       RowBox[{"\[Gamma]m", ",", "\[Alpha]", ",", "\[Mu]"}], "]"}], ")"}], 
     RowBox[{"m", " "}]]}]}], ";", 
  RowBox[{
   RowBox[{"PeMPSK", "[", 
    RowBox[{"\[Gamma]m_", ",", "\[Alpha]_", ",", "\[Mu]_", ",", "N_"}], "]"}],
