   RowBox[{"1", " ", "-", " ", 
    RowBox[{"PNmPSK", "[", 
     RowBox[{"\[Gamma]m", ",", "\[Alpha]", ",", "\[Mu]", ",", "N", ",", "0"}],
      "]"}]}]}], ";"}]], "Input",
